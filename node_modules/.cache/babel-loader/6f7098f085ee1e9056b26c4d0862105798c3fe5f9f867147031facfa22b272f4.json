echarts.init(document.getElementById(\"main1\"));\n    myChart.setOption({\n      title: {\n        text: \"课题组人数分布\",\n      },\n      tooltip: {}, // 鼠标放上去有对应提示\n      xAxis: {\n        data: [\"2018\", \"2019\", \"2020\", \"2021\", \"2022\", \"2023\"],\n      },\n      yAxis: {},\n      // 数据配置在series中\n      series: [\n        {\n          name: \"人数\",\n          // 图表类型，线图line 柱状图bar\n          type: \"bar\",\n          data: [2, 4, 23, 27, 31, 39],\n        },\n      ],\n    });\n  },\n  methods: {\n    draw(legend, xAxis, series) {\n      let myChart1 = this.$echarts.init(document.getElementById(\"main2\"));\n      let option = {\n        title: {\n          text: \"会话量\",\n        },\n        toolbar: {\n          trigger: \"axis\",\n        },\n        legend: {\n          data: legend,\n        },\n        xAxis: {\n          type: \"category\",\n          data: xAxis,\n        },\n        yAxis: {\n          type: \"value\",\n        },\n        series,\n      };\n      myChart1.setOption(option);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n// 图表中一定要给宽高 ， 如果不给宽高会出现代码不报错但是什么都没有\n.dataView {\n  width: 100%;\n  display: flex;\n  justify-content: space-between;\n  .el-card {\n    width: 50%;\n    #main1,\n    #main2 {\n      height: 500px;\n    }\n  }\n}\n</style>\n"],"mappings":"AAYA,SAAAA,QAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;EACA;EACAC,QAAA;EACA;EACAC,QAAA;IACA;IACA,IAAAC,OAAA,QAAAC,QAAA,CAAAC,IAAA,CAAAC,QAAA,CAAAC,cAAA;IACAJ,OAAA,CAAAK,SAAA;MACAC,KAAA;QACAC,IAAA;MACA;MACAC,OAAA;MAAA;MACAC,KAAA;QACAZ,IAAA;MACA;MACAa,KAAA;MACA;MACAC,MAAA,GACA;QACAf,IAAA;QACA;QACAgB,IAAA;QACAf,IAAA;MACA;IAEA;EACA;EACAgB,OAAA;IACAC,KAAAC,MAAA,EAAAN,KAAA,EAAAE,MAAA;MACA,IAAAK,QAAA,QAAAf,QAAA,CAAAC,IAAA,CAAAC,QAAA,CAAAC,cAAA;MACA,IAAAa,MAAA;QACAX,KAAA;UACAC,IAAA;QACA;QACAW,OAAA;UACAC,OAAA;QACA;QACAJ,MAAA;UACAlB,IAAA,EAAAkB;QACA;QACAN,KAAA;UACAG,IAAA;UACAf,IAAA,EAAAY;QACA;QACAC,KAAA;UACAE,IAAA;QACA;QACAD;MACA;MACAK,QAAA,CAAAX,SAAA,CAAAY,MAAA;IACA;EACA;AACA"},"m
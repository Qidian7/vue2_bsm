{"ast":null,"code":"import geoJson from \"@/assets/data.json\";\nexport default {\n  name: \"MapView\",\n  data() {\n    return {};\n  },\n  mounted() {\n    // console.log(geoJson);\n    let myChart = this.$echarts.init(document.getElementById(\"main\"));\n    //  地图的注册,必须包括geo组件和map图表才行\n    this.$echarts.registerMap(\"china\", geoJson);\n    let option = {\n      geo: {\n        map: \"课题组人员家庭住址图\",\n        aspectScale: 0.75,\n        //scale地图长宽比\n        zoom: 1.1,\n        //当前视角缩放比\n        regions: [{\n          name: \"南海诸岛\"\n        }]\n      },\n      series: [{\n        // 配置地图相关参数，绘制地图，这个对象是关于地图图标的相关设置\n        type: \"map\",\n        zoom: 1.1,\n        map: \"课题组人员家庭住址图\"\n      }]\n    };\n    myChart.setOption(option);\n  }\n};","map":{"version":3,"names":["geoJson","name","data","mounted","myChart","$echarts","init","document","getElementById","registerMap","option","geo","map","aspectScale","zoom","regions","series","type","setOption"],"sources":["src/components/DataAnalysis/MapView.vue"],"sourcesContent":["<template>\n  <div class=\"mapView\">\n    <div id=\"main\"></div>\n  </div>\n</template>\n\n<script>\nimport geoJson from \"@/assets/data.json\";\nexport default {\n  name: \"MapView\",\n  data() {\n    return {};\n  },\n  mounted() {\n    // console.log(geoJson);\n    let myChart = this.$echarts.init(document.getElementById(\"main\"));\n    //  地图的注册,必须包括geo组件和map图表才行\n    this.$echarts.registerMap(\"china\", geoJson);\n    let option = {\n      geo: {\n        map: \"课题组人员家庭住址图\",\n        aspectScale: 0.75, //scale地图长宽比\n        zoom: 1.1, //当前视角缩放比\n        regions: [\n          {\n            name: \"南海诸岛\",\n          },\n        ],\n      },\n      series: [\n        {\n          // 配置地图相关参数，绘制地图，这个对象是关于地图图标的相关设置\n          type: \"map\",\n          zoom: 1.1,\n          map: \"课题组人员家庭住址图\",\n        },\n      ],\n    };\n    myChart.setOption(option);\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.mapView {\n  width: 100%;\n 
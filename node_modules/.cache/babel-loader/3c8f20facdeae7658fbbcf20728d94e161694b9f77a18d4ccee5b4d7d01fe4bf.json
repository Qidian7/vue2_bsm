{"ast":null,"code":"import { getTableData } from \"@/utils/table\";\nexport default {\n  name: \"WorkManagement\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      page: 1,\n      size: 10,\n      loading: true // 页面一直转圈在加载\n    };\n  },\n\n  methods: {\n    handleSizeChange(val) {\n      this.size = val;\n      this.page = 1;\n      // 改完之后需重新请求\n      getTableData(this, \"/works\", {\n        page: this.page,\n        size: this.size\n      }, this.tableData);\n    },\n    handleCurrentChange(val) {\n      this.page = val;\n      getTableData(this, \"/works\", {\n        page: this.page,\n        size: this.size\n      }, this.tableData);\n    }\n  },\n  // 后段分页，传入page，size\n  created() {\n    getTableData(this, \"/works\", {\n      page: this.page,\n      size: this.size\n    }, this.tableData);\n  }\n};","map":{"version":3,"names":["getTableData","name","data","tableData","total","page","size","loading","methods","handleSizeChange","val","handleCurrentChange","created"],"sources":["src/components/Student/WorkManagement.vue"],"sourcesContent":["<!-- 分页组件的封装和复用，不复用的在worklist -->\n<template>\n  <div class=\"workManagement\">\n    <el-table :data=\"tableData\" v-loading=\"loading\" border style=\"width: 100%\">\n      <!-- width+180px 改成 align=\"center\" 居中显示去除滚动条 -->\n      <el-table-column prop=\"id\" label=\"用户id\" align=\"center\">\n      </el-table-column>\n      <el-table-column prop=\"userId\" label=\"班级\" align=\"center\">\n      </el-table-column>\n      <el-table-column prop=\"title\" label=\"作业名称\" align=\"center\">\n      </el-table-column>\n      <el-table-column prop=\"completed_text\" label=\"完成情况\" align=\"center\">\n      </el-table-column>\n    </el-table>\n    <el-pagination\n      @size-change=\"handleSizeChange\"\n      @current-change=\"handleCurrentChange\"\n      :current-page=\"page\"\n      :page-sizes=\"[10, 20, 30, 40, 50]\"\n      :page-size=\"size\"\n      layout=\"total, sizes, prev, pager, next, jumper\"\n      :total=\"total\"\n    >\n    </el-pagination>\n  </div>\n</template>\n\n<script>\nimport { getTableData } from \"@/utils/table\";\nexport default {\n  name: \"WorkManagement\",\n  data() {\n    return {\n      tableData: [],\n      total: 0,\n      page: 1,\n      size: 10,\n      loading: true, // 页面一直转圈在加载\n    };\n  },\n  methods: {\n    handleSizeChange(val) {\n      this.size = val;\n      this.page = 1;\n      // 改完之后需重新请求\n      getTableData(\n        this,\n        \"/works\",\n        { page: this.page, size: this.size },\n        this.tableData\n      );\n    },\n    handleCurrentChange(val) {\n      this.page = val;\n      getTableData(\n        this,\n        \"/works\",\n        { page: this.page, size: this.size },\n        this.tableData\n      );\n    },\n  },\n  // 后段分页，传入page，size\n  created() {\n    getTableData(\n      this,\n      \"/works\",\n      { page: this.page, size: this.size },\n      this.tableData\n    );\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.workManagement {\n  .el-pagination {\n    text-align: left;\n    margin-top: 20px;\n  }\n}\n</style>\n"],"mappings":"AA4BA,SAAAA,YAAA;AACA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,IAAA;MACAC,IAAA;MACAC,OAAA;IACA;EACA;;EACAC,OAAA;IACAC,iBAAAC,GAAA;MACA,KAAAJ,IAAA,GAAAI,GAAA;MACA,KAAAL,IAAA;MACA;MACAL,YAAA,CACA,MACA,UACA;QAAAK,IAAA,OAAAA,IAAA;QAAAC,IAAA,OAAAA;MAAA,GACA,KAAAH,SAAA,CACA;IACA;IACAQ,oBAAAD,GAAA;MACA,KAAAL,IAAA,GAAAK,GAAA;MACAV,YAAA,CACA,MACA,UACA;QAAAK,IAAA,OAAAA,IAAA;QAAAC,IAAA,OAAAA;MAAA,GACA,KAAAH,SAAA,CACA;IACA;EACA;EACA;EACAS,QAAA;IACAZ,YAAA,CACA,MACA,UACA;MAAAK,IAAA,OAAAA,IAAA;MAAAC,IAAA,OAAAA;IAAA,GACA,KAAAH,SAAA,CACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}
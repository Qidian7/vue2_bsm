model=\"form.phone\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"closeInfo('form')\"> 取 消 </el-button>\n        <el-button type=\"primary\" @click=\"sure('form')\"> 确 定 </el-button>\n      </div>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"InfoManagement\",\n  data() {},\n  methods: {\n    addStudent() {},\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped></style>\n"],"mappings":"AAgHA;EACAA,IAAA;EACAC,KAAA;EACAC,OAAA;IACAC,WAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}